void rightrotation(int arr[],int n){
    
    int temp = arr[n-1], j;
        for (j = n-1; j > 0; j--)
            arr[j] = arr[j-1];
        arr[0] = temp;
}

void left_rotate_by_one(int arr[], int n)
{
    int temp = arr[0], i;
    for (i = 0; i < n - 1; i++)
      arr[i] = arr[i + 1];
    arr[i] = temp;
}
